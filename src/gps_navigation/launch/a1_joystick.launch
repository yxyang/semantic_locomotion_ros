<launch>
  <include file="$(find gamepad_controller)/launch/gamepad_sim.launch" />
  <node pkg="gamepad_controller" type="gamepad_reader.py" name="gamepad_controller" respawn="true"/>
  <node pkg="gps_navigation" type="pointmass_robot.py" name="pointmass_robot" respawn="false" args="--gps_anchor_file=/home/yxyang/research/semantic_locomotion_ros/src/gps_navigation/data/around_lawn.npy"/>
  <node pkg="gps_navigation" type="path_generator.py" name="path_generator" respawn="false" args="--waypoint_file_path=/home/yxyang/research/semantic_locomotion_ros/src/gps_navigation/data/around_lawn.npy"/>
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find gps_navigation)/rviz/pointmass.rviz" />
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" >
    <arg name="port" value="9090"/>
  </include>
  <node pkg="info_publisher" type="web_server.py" name="web_server" />
</launch>
