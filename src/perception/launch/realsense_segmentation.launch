  <node pkg="perception" type="fixed_region_mapper.py" name="fixed_region_mapper" />
  <node pkg="image_transport" type="republish" name="republish_segmentation" args="raw in:=/perception/segmentation_map compressed out:=/perception/segmentation_map_repub" />
